<h1> Blogger#new </h1>

<% if flash[:blogger] %>
<% flash[:blogger].each do |b|%>
<%= b %>
<% end %>
<% end %>
<br>
<%= form_for @blogger do |f| %>
<br>
<%= f.label :name %>
<%= f.text_field :name %>
<br>
<%= f.label :bio %>
<%= f.text_field :bio %>
<br>
<%= f.label :age %>
<%= f.text_field :age %>
<br>
<%= f.submit %>
<% end %>